  <define name="case_vyse42_tokai">
    <element name="jb:Head">
      <ref name="head_common"/>
      <element name="jb:InfoKind">
        <value>東海地震関連情報</value>
      </element>
      <element name="jb:InfoKindVersion">
        <data type="token"/>
      </element>
      <element name="jb:Headline">
        <element name="jb:Text">
          <text/>
        </element>
      </element>
    </element>
    <element name="js:Body">
      <element name="js:Tokai">
        <ref name="anyContent"/>
      </element>
      <optional>
        <element name="js:NextAdvisory">
          <text/>
        </element>
      </optional>
