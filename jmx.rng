<?xml version="1.0" encoding="UTF-8"?>
<!--
  名前空間接頭辞は公式ドキュメントと異なりとにかく短くした。
  
  防災XMLトップ (/jx:Report) 及び (/jx:Report/jx:Control) 以下で用いられる
-->
<!-- /jx:Report/jb:Head 以下で用いられる -->
<!--
  /jx:Report の第3子要素は jm:Body, js:Body, または jv:Body であり、
  地震分野で js:, 火山分野で jv:, その他大気海洋関係で jm: が用いられる。
-->
<!-- jm:*, js:*, または jv:* の下で共通に用いられる要素。物理量の表現が多い。 -->
<grammar xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0" xmlns:jm="http://xml.kishou.go.jp/jmaxml1/body/meteorology1/" xmlns:jb="http://xml.kishou.go.jp/jmaxml1/informationBasis1/" xmlns:js="http://xml.kishou.go.jp/jmaxml1/body/seismology1/" xmlns:je="http://xml.kishou.go.jp/jmaxml1/elementBasis1/" xmlns:jv="http://xml.kishou.go.jp/jmaxml1/body/volcanology1/" xmlns:jx="http://xml.kishou.go.jp/jmaxml1/" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://relaxng.org/ns/structure/1.0" datatypeLibrary="http://www.w3.org/2001/XMLSchema-datatypes">
  <start>
    <a:documentation>気象庁防災XMLはすべて js:Report をルート要素とする。
最初の2つの子要素は必ず jx:Control 及び jb:Head であり、ほとんどの
情報種別で同じ構造を持つ。</a:documentation>
    <element name="jx:Report">
      <element name="jx:Control">
        <element name="jx:Title">
          <ref name="list_title"/>
        </element>
        <element name="jx:DateTime">
          <ref name="dateTime_sz"/>
        </element>
        <element name="jx:Status">
          <choice>
            <value>通常</value>
            <value>訓練</value>
            <value>試験</value>
          </choice>
        </element>
        <element name="jx:EditorialOffice">
          <ref name="list_edof"/>
        </element>
        <element name="jx:PublishingOffice">
          <ref name="list_pbof"/>
        </element>
      </element>
      <choice>
        <ref name="case_vgsk50_kisetsu"/>
        <ref name="case_vgsk55_seibutsu"/>
        <ref name="case_vgsk60_tokushu"/>
        <ref name="case_vguv50_shigaisen"/>
        <ref name="case_vpti50_joho"/>
        <ref name="case_vptw40_typhoon"/>
        <ref name="case_vpzu50_kaijou"/>
        <ref name="case_vpcu50_kaijou"/>
        <ref name="case_vpcy50_umiyoho"/>
        <ref name="case_vpww50_keihou"/>
        <ref name="case_vxko_kasen"/>
        <ref name="case_vxko_emptybody"/>
        <ref name="case_vxww50_dosha"/>
        <ref name="case_vpoa50_kirokuame"/>
        <ref name="case_vphw50_tatsuchu"/>
        <ref name="case_vpfg50_gaikyou"/>
        <ref name="case_vpfd50_fuken"/>
        <ref name="case_vpfw50_week"/>
        <ref name="case_vpzi50_tenkou"/>
        <ref name="case_vxse51_shindo"/>
        <ref name="case_vxse52_shingen"/>
        <ref name="case_vxse53_joho"/>
        <ref name="case_vxse56_katsudou"/>
        <ref name="case_vxse60_kaisuu"/>
        <ref name="case_vxse61_koushin"/>
        <ref name="case_vtse40_tsunamikeiho"/>
        <ref name="case_vtse50_tsunamijoho"/>
        <ref name="case_vxse41_eew"/>
        <ref name="case_vyse42_tokai"/>
        <ref name="case_vzse40_oshirase"/>
        <ref name="case_vzvo40_oshirase"/>
        <ref name="case_vfvo50_funka"/>
        <ref name="case_vfvo52_kazanobs"/>
        <ref name="case_vpzk50_kisetsu"/>
        <ref name="case_vpaw50_sokei"/>
        <ref name="case_vzsa50_tenkizu"/>
        <ref name="case_vpno50_tokkei"/>
        <ref name="case_vfvo53_ashfall"/>
        <ref name="case_vfvo56_funsoku"/>
        <ref name="case_vpfd60_sochu"/>
        <ref name="case_vpck70_fortnight"/>
        <ref name="case_vpaw51_soten"/>
      </choice>
    </element>
  </start>
  <define name="head_common">
    <element name="jb:Title">
      <data type="token"/>
    </element>
    <element name="jb:ReportDateTime">
      <ref name="dateTime_si"/>
    </element>
    <element name="jb:TargetDateTime">
      <choice>
        <attribute name="xsi:nil">
          <value>true</value>
        </attribute>
        <ref name="dateTime_si"/>
      </choice>
    </element>
    <optional>
      <element name="jb:TargetDTDubious">
        <value>頃</value>
      </element>
    </optional>
    <optional>
      <element name="jb:TargetDuration">
        <data type="duration"/>
      </element>
    </optional>
    <optional>
      <element name="jb:ValidDateTime">
        <ref name="dateTime_si"/>
      </element>
    </optional>
    <element name="jb:EventID">
      <text/>
    </element>
    <element name="jb:InfoType">
      <choice>
        <value>発表</value>
        <value>訂正</value>
        <value>取消</value>
      </choice>
    </element>
    <element name="jb:Serial">
      <choice>
        <empty/>
        <data type="token"/>
      </choice>
    </element>
  </define>
  <!--
    === Head and Body rules ===
    - - - rules using jm:Body （大気海洋系情報）
    
  -->
  <define name="case_vpck70_fortnight">
    <element name="jb:Head">
      <ref name="head_common"/>
      <element name="jb:InfoKind">
        <value>２週間気温予報</value>
      </element>
      <element name="jb:InfoKindVersion">
        <data type="token"/>
      </element>
      <element name="jb:Headline">
        <element name="jb:Text">
          <empty/>
        </element>
      </element>
    </element>
    <element name="jm:Body">
      <optional>
        <element name="jm:MeteorologicalInfos">
          <attribute name="type">
            <value>区域予報</value>
          </attribute>
          <oneOrMore>
            <ref name="anyElement"/>
          </oneOrMore>
        </element>
      </optional>
      <optional>
        <element name="jm:MeteorologicalInfos">
          <attribute name="type">
            <value>地点予報</value>
          </attribute>
          <oneOrMore>
            <ref name="anyElement"/>
          </oneOrMore>
        </element>
      </optional>
    </element>
  </define>
