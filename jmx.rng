      <ref name="head_common"/>
      <element name="jb:InfoKind">
        <value>異常天候早期警戒情報</value>
      </element>
      <element name="jb:InfoKindVersion">
        <data type="token"/>
      </element>
      <element name="jb:Headline">
        <element name="jb:Text">
          <text/>
        </element>
        <element name="jb:Information">
          <attribute name="type">
            <value>異常天候早期警戒情報</value>
          </attribute>
          <oneOrMore>
            <ref name="anyElement"/>
          </oneOrMore>
        </element>
      </element>
    </element>
    <element name="jm:Body">
      <element name="jm:TargetArea">
        <attribute name="codeType">
          <value>全国・地方予報区等</value>
        </attribute>
        <element name="jm:Name">
          <data type="token"/>
        </element>
        <element name="jm:Code">
          <data type="positiveInteger">
            <param name="totalDigits">6</param>
          </data>
        </element>
      </element>
      <optional>
        <element name="jm:Notice">
          <text/>
        </element>
      </optional>
      <element name="jm:MeteorologicalInfos">
        <attribute name="type">
          <value>異常天候早期警戒情報</value>
        </attribute>
        <oneOrMore>
          <ref name="anyElement"/>
        </oneOrMore>
      </element>
    </element>
  </define>
