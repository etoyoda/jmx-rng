  </define>
  <define name="case_vxse41_eew">
    <element name="jb:Head">
      <ref name="head_common"/>
      <element name="jb:InfoKind">
        <value>緊急地震速報</value>
      </element>
      <element name="jb:InfoKindVersion">
        <data type="token"/>
      </element>
      <element name="jb:Headline">
        <element name="jb:Text">
          <text/>
        </element>
        <zeroOrMore>
          <element name="jb:Information">
            <attribute name="type"/>
            <oneOrMore>
              <!--
                <Information type="緊急地震速報（地方予報区）">
                <Information type="緊急地震速報（府県予報区）">
                <Information type="緊急地震速報（細分区域）">
              -->
              <element name="jb:Item">
                <element name="jb:Kind">
                  <oneOrMore>
                    <ref name="anyElement"/>
                  </oneOrMore>
                </element>
                <element name="jb:LastKind">
                  <oneOrMore>
                    <ref name="anyElement"/>
                  </oneOrMore>
                </element>
                <element name="jb:Areas">
                  <attribute name="codeType"/>
                  <oneOrMore>
                    <element name="jb:Area">
                      <element name="jb:Name">
                        <data type="token"/>
                      </element>
                      <element name="jb:Code">
                        <data type="token"/>
                      </element>
                    </element>
                  </oneOrMore>
                </element>
              </element>
            </oneOrMore>
          </element>
        </zeroOrMore>
      </element>
    </element>
    <element name="js:Body">
      <ref name="anyContent"/>
    </element>
  </define>
  <define name="case_vtse50_tsunamijoho">
