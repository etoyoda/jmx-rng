  <define name="case_vpzk50_kisetsu">
    <element name="jb:Head">
      <ref name="head_common"/>
      <element name="jb:InfoKind">
        <value>季節予報</value>
      </element>
      <element name="jb:InfoKindVersion">
        <data type="token"/>
      </element>
      <element name="jb:Headline">
        <element name="jb:Text">
