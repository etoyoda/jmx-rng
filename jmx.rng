      </attribute>
      <oneOrMore>
        <ref name="anyElement"/>
      </oneOrMore>
    </element>
    <optional>
      <element name="jb:Information">
        <attribute name="type">
          <value>気象特別警報報知（一次細分区域等）</value>
        </attribute>
        <oneOrMore>
          <ref name="anyElement"/>
        </oneOrMore>
      </element>
    </optional>
    <optional>
      <element name="jb:Information">
        <attribute name="type">
          <value>気象特別警報報知（市町村等をまとめた地域等）</value>
        </attribute>
        <oneOrMore>
          <ref name="anyElement"/>
        </oneOrMore>
      </element>
    </optional>
    <optional>
      <element name="jb:Information">
        <attribute name="type">
          <value>気象特別警報報知（市町村等）</value>
        </attribute>
        <oneOrMore>
          <ref name="anyElement"/>
        </oneOrMore>
      </element>
    </optional>
    <optional>
      <element name="jb:Information">
        <attribute name="type">
          <value>気象特別警報報知（警報注意報種別毎）</value>
        </attribute>
        <oneOrMore>
          <ref name="anyElement"/>
        </oneOrMore>
      </element>
    </optional>
  </define>
  <!-- may be missing in case of lifting -->
  <define name="case_vpno50_tokkei">
    <element name="jb:Head">
      <ref name="head_common"/>
      <element name="jb:InfoKind">
        <value>気象特別警報報知</value>
      </element>
      <element name="jb:InfoKindVersion">
        <data type="token"/>
      </element>
      <element name="jb:Headline">
        <element name="jb:Text">
          <data type="string"/>
        </element>
        <ref name="infos_vpno50_tokkei"/>
      </element>
    </element>
    <element name="jm:Body">
      <empty/>
    </element>
  </define>
  <define name="case_vzsa50_tenkizu">
    <element name="jb:Head">
      <ref name="head_common"/>
      <element name="jb:InfoKind">
        <value>天気図情報</value>
      </element>
      <element name="jb:InfoKindVersion">
        <data type="token"/>
      </element>
      <element name="jb:Headline">
        <element name="jb:Text">
          <empty/>
        </element>
      </element>
    </element>
    <element name="jm:Body">
      <element name="jm:MeteorologicalInfos">
        <attribute name="type">
          <value>天気図情報</value>
        </attribute>
        <element name="jm:MeteorologicalInfo">
          <element name="jm:DateTime">
            <attribute name="type"/>
            <ref name="dateTime_si"/>
          </element>
          <oneOrMore>
            <element name="jm:Item">
              <element name="jm:Kind">
                <choice>
                  <ref name="tenkizu_isobar"/>
                  <ref name="tenkizu_hilo"/>
                  <ref name="tenkizu_zensen"/>
                </choice>
              </element>
            </element>
          </oneOrMore>
        </element>
      </element>
      <optional>
        <element name="jm:MeteorologicalInfos">
          <attribute name="type">
            <value>悪天情報</value>
          </attribute>
          <element name="jm:MeteorologicalInfo">
            <element name="jm:DateTime">
              <attribute name="type"/>
              <ref name="dateTime_si"/>
            </element>
            <oneOrMore>
              <element name="jm:Item">
                <choice>
                  <ref name="tenkizu_polygon"/>
                  <ref name="tenkizu_areacode"/>
                  <ref name="tenkizu_hatch"/>
                  <ref name="tenkizu_pointwind"/>
                </choice>
              </element>
            </oneOrMore>
          </element>
        </element>
      </optional>
    </element>
  </define>
  <define name="tenkizu_isobar">
    <element name="jm:Property">
      <element name="jm:Type">
        <value>等圧線</value>
      </element>
      <element name="jm:IsobarPart">
        <element name="je:Pressure">
          <attribute name="type">
            <value>気圧</value>
          </attribute>
          <attribute name="unit">
            <value>hPa</value>
          </attribute>
          <data type="positiveInteger"/>
        </element>
        <element name="je:Line">
          <attribute name="type">
            <value>位置（度）</value>
          </attribute>
          <ref name="latlon_decimal2_multi_ascii"/>
        </element>
      </element>
    </element>
  </define>
  <define name="tenkizu_hilo">
    <element name="jm:Property">
      <element name="jm:Type">
        <choice>
          <value>低気圧</value>
          <value>高気圧</value>
          <value>熱帯低気圧</value>
          <value>低圧部</value>
        </choice>
      </element>
      <element name="jm:CenterPart">
        <oneOrMore>
          <ref name="anyElement"/>
        </oneOrMore>
      </element>
    </element>
  </define>
  <define name="tenkizu_zensen">
    <element name="jm:Property">
      <element name="jm:Type">
        <choice>
          <value>寒冷前線</value>
          <value>温暖前線</value>
          <value>閉塞前線</value>
          <value>停滞前線</value>
        </choice>
      </element>
      <element name="jm:CoordinatePart">
        <element name="je:Line">
          <attribute name="type">
            <value>前線（度）</value>
          </attribute>
          <ref name="latlon_decimal2_multi_ascii"/>
        </element>
      </element>
    </element>
  </define>
  <define name="tenkizu_polygon">
    <element name="jm:Kind">
      <element name="jm:Name">
        <value>悪天情報（霧）</value>
      </element>
    </element>
    <element name="jm:Area">
      <element name="jm:Name">
        <value>霧域</value>
      </element>
      <element name="je:Polygon">
        <attribute name="type">
          <value>位置（度）</value>
        </attribute>
        <ref name="latlon_multi_ascii"/>
      </element>
    </element>
  </define>
  <define name="tenkizu_areacode">
    <element name="jm:Kind">
      <element name="jm:Name">
        <value>悪天情報（霧）</value>
      </element>
    </element>
    <element name="jm:Area">
      <attribute name="codeType">
        <value>全般海上海域名</value>
      </attribute>
      <element name="jm:Name">
        <data type="token"/>
      </element>
      <element name="jm:Code">
        <data type="positiveInteger">
          <param name="totalDigits">4</param>
        </data>
      </element>
    </element>
  </define>
  <define name="tenkizu_hatch">
    <element name="jm:Kind">
      <element name="jm:Name">
        <choice>
          <value>悪天情報（海氷）</value>
          <value>悪天情報（船体着氷）</value>
        </choice>
      </element>
    </element>
    <element name="jm:Area">
      <element name="jm:Name">
        <choice>
          <value>海氷域</value>
          <value>海氷</value>
          <value>船体着氷域</value>
        </choice>
      </element>
      <element name="je:Coordinate">
        <attribute name="type">
          <value>領域（度）</value>
        </attribute>
        <ref name="latlon_decimal1_multi_ascii"/>
      </element>
    </element>
  </define>
  <define name="tenkizu_pointwind">
    <element name="jm:Kind">
      <element name="jm:Name">
        <value>悪天情報（強風）</value>
      </element>
      <element name="jm:Property">
        <element name="jm:Type">
          <value>風</value>
        </element>
        <element name="jm:WindPart">
          <element name="je:WindDegree">
            <attribute name="type">
              <value>風向</value>
            </attribute>
            <attribute name="unit">
              <value>度（真方位）</value>
            </attribute>
            <data type="positiveInteger"/>
          </element>
          <element name="je:WindSpeed">
            <attribute name="type">
              <value>最大風速</value>
            </attribute>
            <attribute name="unit">
              <value>ノット</value>
            </attribute>
            <data type="positiveInteger"/>
          </element>
        </element>
      </element>
    </element>
    <element name="jm:Area">
      <element name="jm:Name">
        <value>強風域</value>
      </element>
      <element name="je:Coordinate">
        <attribute name="type">
          <value>位置（度）</value>
        </attribute>
        <ref name="latlon_decimal_ascii"/>
      </element>
    </element>
  </define>
  <define name="case_vpaw50_sokei">
