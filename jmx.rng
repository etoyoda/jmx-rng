<?xml version="1.0" encoding="UTF-8"?>
<grammar ns="http://xml.kishou.go.jp/jmaxml1/elementBasis1/" xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0" xmlns:jm="http://xml.kishou.go.jp/jmaxml1/body/meteorology1/" xmlns:jb="http://xml.kishou.go.jp/jmaxml1/informationBasis1/" xmlns:je="http://xml.kishou.go.jp/jmaxml1/elementBasis1/" xmlns:jx="http://xml.kishou.go.jp/jmaxml1/" xmlns="http://relaxng.org/ns/structure/1.0" datatypeLibrary="http://www.w3.org/2001/XMLSchema-datatypes">
  <start>
    <element name="jx:Report">
      <element name="jx:Control">
        <element name="jx:Title">
          <ref name="list_title"/>
        </element>
        <element name="jx:DateTime">
          <ref name="dateTime_sz"/>
        </element>
        <element name="jx:Status">
          <choice>
            <value>通常</value>
            <value>訓練</value>
            <value>試験</value>
          </choice>
        </element>
        <element name="jx:EditorialOffice">
          <ref name="list_edof"/>
        </element>
        <element name="jx:PublishingOffice">
          <ref name="list_pbof"/>
        </element>
      </element>
      <element name="jb:Head">
        <element name="jb:Title">
          <data type="token"/>
        </element>
        <!-- 発表時刻 -->
        <element name="jb:ReportDateTime">
          <ref name="dateTime_si"/>
        </element>
        <!-- 解析時刻 -->
        <element name="jb:TargetDateTime">
          <ref name="dateTime_si"/>
        </element>
        <optional>
          <element name="jb:TargetDuration">
            <data type="duration"/>
          </element>
        </optional>
        <optional>
          <!-- yuukou kigen -->
          <element name="jb:ValidDateTime">
            <ref name="dateTime_si"/>
          </element>
        </optional>
        <!-- TC番号 -->
        <element name="jb:EventID">
          <choice>
            <empty/>
            <data type="token"/>
          </choice>
        </element>
        <element name="jb:InfoType">
          <choice>
            <value>発表</value>
            <value>訂正</value>
            <value>取消</value>
          </choice>
        </element>
        <!-- 情報番号というが起算・加算条件は未詳 -->
        <element name="jb:Serial">
          <choice>
            <empty/>
            <data type="token"/>
          </choice>
        </element>
        <element name="jb:InfoKind">
          <ref name="list_infokind"/>
        </element>
        <!-- we can simply ignore this -->
        <element name="jb:InfoKindVersion">
          <data type="token"/>
        </element>
        <element name="jb:Headline">
          <element name="jb:Text">
            <data type="string"/>
          </element>
          <choice>
            <empty/>
            <ref name="info_vpzu50_kaijou"/>
            <ref name="info_vpzu50_kaijou_clear"/>
            <ref name="info_vpcu50_kaijou"/>
            <ref name="infos_vpww50_keihou"/>
          </choice>
        </element>
      </element>
      <choice>
        <ref name="body_vgsk50_kisetsu"/>
        <ref name="body_vgsk55_seibutsu"/>
        <ref name="body_vgsk60_tokushu"/>
        <ref name="body_vguv50_shigaisen"/>
        <ref name="body_vpti50_joho"/>
        <ref name="body_vptw40_typhoon"/>
        <ref name="body_vpzu50_kaijou"/>
        <ref name="body_vpcu50_kaijou"/>
        <ref name="body_vpcy50_umiyoho"/>
        <ref name="body_vpww50_keihou"/>
      </choice>
    </element>
  </start>
  <!--
    
    - - - jb:Information rules - - -
    
  -->
  <define name="infos_vpww50_keihou">
    <element name="jb:Information">
      <attribute name="type">
        <value>気象警報・注意報（府県予報区等）</value>
      </attribute>
      <oneOrMore>
        <ref name="anyElement"/>
      </oneOrMore>
    </element>
    <element name="jb:Information">
      <attribute name="type">
        <value>気象警報・注意報（一次細分区域等）</value>
      </attribute>
      <oneOrMore>
        <ref name="anyElement"/>
      </oneOrMore>
    </element>
    <element name="jb:Information">
      <attribute name="type">
        <value>気象警報・注意報（市町村等をまとめた地域等）</value>
      </attribute>
      <oneOrMore>
        <ref name="anyElement"/>
      </oneOrMore>
    </element>
    <element name="jb:Information">
      <attribute name="type">
        <value>気象警報・注意報（市町村等）</value>
      </attribute>
      <oneOrMore>
        <ref name="anyElement"/>
      </oneOrMore>
    </element>
    <optional>
      <element name="jb:Information">
        <attribute name="type">
          <value>気象警報・注意報（警報注意報種別毎）</value>
        </attribute>
        <oneOrMore>
          <ref name="anyElement"/>
        </oneOrMore>
      </element>
    </optional>
  </define>
  <!-- may be missing in case of lifting -->
  <define name="info_vpzu50_kaijou">
    <element name="jb:Information">
      <attribute name="type">
        <value>全般海上警報</value>
      </attribute>
      <oneOrMore>
        <element name="jb:Item">
          <element name="jb:Kind">
            <element name="jb:Name">
              <ref name="list_kaijou_keihoumei"/>
            </element>
            <element name="jb:Code">
              <ref name="list_kaijou_keihoucode"/>
            </element>
          </element>
          <element name="jb:Areas">
            <attribute name="codeType">
              <value>全般海上海域名</value>
            </attribute>
            <choice>
              <ref name="kaijou_area_circles"/>
              <ref name="kaijou_area_point"/>
              <ref name="kaijou_area_polygon"/>
              <ref name="kaijou_area_codes"/>
            </choice>
          </element>
        </element>
      </oneOrMore>
    </element>
  </define>
  <define name="info_vpcu50_kaijou">
    <element name="jb:Information">
      <attribute name="type">
        <value>地方海上警報</value>
      </attribute>
      <oneOrMore>
        <element name="jb:Item">
          <element name="jb:Kind">
            <element name="jb:Name">
              <ref name="list_kaijou_keihoumei"/>
            </element>
            <element name="jb:Code">
              <ref name="list_kaijou_keihoucode"/>
            </element>
          </element>
          <element name="jb:Areas">
            <attribute name="codeType">
              <value>地方海上予報区</value>
            </attribute>
            <ref name="kaijou_area_codes"/>
          </element>
        </element>
      </oneOrMore>
    </element>
  </define>
  <define name="kaijou_area_circles">
    <element name="jb:Area">
      <element name="jb:Name">
        <choice>
          <value>低気圧</value>
          <value>熱帯低気圧</value>
        </choice>
      </element>
      <oneOrMore>
        <element name="je:Circle">
          <attribute name="type">
            <value>影響範囲</value>
          </attribute>
          <optional>
            <!-- either analysis or 24 hour forecast present -->
            <element name="je:BasePoint">
              <attribute name="type">
                <value>実況位置（度）</value>
              </attribute>
              <ref name="kaijou_point"/>
            </element>
          </optional>
          <optional>
            <element name="je:BasePoint">
              <attribute name="type">
                <value>１２時間後位置（度）</value>
              </attribute>
              <ref name="kaijou_point"/>
            </element>
          </optional>
          <optional>
            <element name="je:BasePoint">
              <attribute name="type">
                <value>２４時間後位置（度）</value>
              </attribute>
              <optional>
                <ref name="kaijou_point"/>
              </optional>
            </element>
          </optional>
          <optional>
            <ref name="radii_of_wind"/>
          </optional>
        </element>
      </oneOrMore>
    </element>
  </define>
  <define name="kaijou_point">
    <choice>
      <group>
        <attribute name="description">
          <ref name="latlon_deg_ja"/>
        </attribute>
        <ref name="latlon_deg_ascii"/>
      </group>
      <group>
        <attribute name="description">
          <ref name="latlon_decimal_ja"/>
        </attribute>
        <ref name="latlon_decimal_ascii"/>
      </group>
    </choice>
  </define>
  <define name="kaijou_area_point">
    <element name="jb:Area">
      <element name="jb:Name">
        <value>熱帯低気圧</value>
      </element>
      <element name="je:Coordinate">
        <attribute name="type">
          <choice>
            <value>中心位置（度）</value>
            <value>実況位置（度）</value>
          </choice>
        </attribute>
        <attribute name="description">
          <ref name="latlon_decimal_ja"/>
        </attribute>
        <ref name="latlon_decimal_ascii"/>
      </element>
    </element>
  </define>
  <define name="kaijou_area_codes">
    <oneOrMore>
      <element name="jb:Area">
        <element name="jb:Name">
          <data type="token"/>
        </element>
        <element name="jb:Code">
          <data type="positiveInteger"/>
        </element>
      </element>
    </oneOrMore>
  </define>
  <define name="kaijou_area_polygon">
    <element name="jb:Area">
      <element name="jb:Name">
        <value>緯度経度領域</value>
      </element>
      <element name="je:Polygon">
        <attribute name="type">
          <value>位置（度）</value>
        </attribute>
        <attribute name="description">
          <ref name="latlon_multi_ja"/>
        </attribute>
        <ref name="latlon_multi_ascii"/>
      </element>
    </element>
  </define>
  <define name="info_vpzu50_kaijou_clear">
    <element name="jb:Information">
      <attribute name="type">
        <value>全般海上警報</value>
      </attribute>
      <element name="jb:Item">
        <element name="jb:Kind">
          <element name="jb:Name">
            <value>海上警報なし</value>
          </element>
          <!-- this time jb:Code missing -->
        </element>
        <element name="jb:Areas">
          <attribute name="codeType">
            <value>全般海上海域名</value>
          </attribute>
          <element name="jb:Area">
            <element name="jb:Name">
              <value>全般海上予報区</value>
            </element>
          </element>
        </element>
      </element>
    </element>
  </define>
  <!-- === Body rules === -->
  <define name="body_vpww50_keihou">
    <element name="jm:Body">
      <zeroOrMore>
        <element name="jm:Notice">
          <text/>
        </element>
      </zeroOrMore>
      <element name="jm:Warning">
        <attribute name="type">
          <value>気象警報・注意報（府県予報区等）</value>
        </attribute>
        <oneOrMore>
          <ref name="anyElement"/>
        </oneOrMore>
      </element>
      <element name="jm:Warning">
        <attribute name="type">
          <value>気象警報・注意報（一次細分区域等）</value>
        </attribute>
        <oneOrMore>
          <ref name="anyElement"/>
        </oneOrMore>
      </element>
      <element name="jm:Warning">
        <attribute name="type">
          <value>気象警報・注意報（市町村等をまとめた地域等）</value>
        </attribute>
        <oneOrMore>
          <ref name="anyElement"/>
        </oneOrMore>
      </element>
      <element name="jm:Warning">
        <attribute name="type">
          <value>気象警報・注意報（市町村等）</value>
        </attribute>
        <oneOrMore>
          <ref name="anyElement"/>
        </oneOrMore>
      </element>
      <optional>
        <element name="jm:MeteorologicalInfos">
          <attribute name="type">
            <value>量的予想時系列（市町村等）</value>
          </attribute>
          <oneOrMore>
            <ref name="anyElement"/>
          </oneOrMore>
        </element>
      </optional>
    </element>
  </define>
  <define name="body_vpcy50_umiyoho">
    <element name="jm:Body">
      <element name="jm:Warning">
        <attribute name="type">
          <value>地方海上警報発表状況</value>
        </attribute>
        <oneOrMore>
          <element name="jm:Item">
            <oneOrMore>
              <element name="jm:Kind">
                <choice>
                  <group>
                    <element name="jm:Name">
                      <data type="token"/>
                    </element>
                    <element name="jm:Code">
                      <data type="nonNegativeInteger"/>
                    </element>
                  </group>
                  <element name="jm:Status">
                    <value>海上警報なし</value>
                  </element>
                </choice>
              </element>
            </oneOrMore>
            <element name="jm:Area">
              <element name="jm:Name">
                <data type="token"/>
              </element>
              <element name="jm:Code">
                <data type="nonNegativeInteger"/>
              </element>
            </element>
          </element>
        </oneOrMore>
      </element>
      <ref name="kaijou_kishouyouin"/>
      <ref name="umiyoho_jikkyou"/>
      <ref name="umiyoho_kaiiki"/>
    </element>
  </define>
  <define name="umiyoho_jikkyou">
    <element name="jm:MeteorologicalInfos">
      <attribute name="type">
        <value>観測実況</value>
      </attribute>
      <oneOrMore>
        <ref name="anyElement"/>
      </oneOrMore>
    </element>
  </define>
  <define name="umiyoho_kaiiki">
    <element name="jm:MeteorologicalInfos">
      <attribute name="type">
        <value>地方海域の予報</value>
      </attribute>
      <oneOrMore>
        <element name="jm:MeteorologicalInfo">
          <element name="jm:DateTime">
            <ref name="dateTime_si"/>
          </element>
          <element name="jm:Duration">
            <data type="duration"/>
          </element>
          <!-- name of forecast time range duration -->
          <element name="jm:Name">
            <data type="token"/>
          </element>
          <oneOrMore>
            <element name="jm:Item">
              <oneOrMore>
                <element name="jm:Kind">
                  <optional>
                    <choice>
                      <ref name="kaijou_propwind"/>
                      <ref name="umiyoho_tenki"/>
                      <ref name="umiyoho_propfog"/>
                      <ref name="kaijou_propuneri"/>
                      <ref name="umiyoho_propice"/>
                    </choice>
                  </optional>
                </element>
              </oneOrMore>
              <element name="jm:Area">
                <element name="jm:Name">
                  <data type="token"/>
                </element>
                <element name="jm:Code">
                  <data type="nonNegativeInteger"/>
                </element>
              </element>
            </element>
          </oneOrMore>
        </element>
      </oneOrMore>
    </element>
  </define>
  <define name="umiyoho_tenki">
    <element name="jm:Property">
      <element name="jm:Type">
        <value>天気</value>
      </element>
      <element name="jm:WeatherPart">
        <oneOrMore>
          <ref name="anyElement"/>
        </oneOrMore>
      </element>
    </element>
  </define>
  <define name="umiyoho_propfog">
    <element name="jm:Property">
      <element name="jm:Type">
        <value>視程</value>
      </element>
      <element name="jm:VisibilityPart">
        <oneOrMore>
          <ref name="anyElement"/>
        </oneOrMore>
      </element>
    </element>
  </define>
  <define name="umiyoho_propice">
    <element name="jm:Property">
      <element name="jm:Type">
        <value>流氷</value>
      </element>
      <element name="jm:SeaIcePart">
        <oneOrMore>
          <ref name="anyElement"/>
        </oneOrMore>
      </element>
    </element>
  </define>
  <define name="body_vpcu50_kaijou">
    <element name="jm:Body">
      <element name="jm:Warning">
        <attribute name="type">
          <value>地方海上警報</value>
        </attribute>
        <oneOrMore>
          <element name="jm:Item">
            <oneOrMore>
              <element name="jm:Kind">
                <optional>
                  <element name="jm:Name">
                    <data type="token"/>
                  </element>
                </optional>
                <optional>
                  <element name="jm:Code">
                    <data type="nonNegativeInteger"/>
                  </element>
                </optional>
                <optional>
                  <choice>
                    <ref name="kaijou_propwind"/>
                    <ref name="kaijou_propfog"/>
                    <ref name="kaijou_propuneri"/>
                    <ref name="kaijou_propicing"/>
                  </choice>
                </optional>
              </element>
            </oneOrMore>
            <element name="jm:Area">
              <element name="jm:Name">
                <data type="token"/>
              </element>
              <element name="jm:Code">
                <data type="nonNegativeInteger"/>
              </element>
            </element>
          </element>
        </oneOrMore>
      </element>
      <ref name="kaijou_kishouyouin"/>
    </element>
  </define>
  <define name="kaijou_propwind">
    <element name="jm:Property">
      <element name="jm:Type">
        <value>風</value>
      </element>
      <element name="jm:WindPart">
        <oneOrMore>
          <ref name="anyElement"/>
        </oneOrMore>
      </element>
    </element>
  </define>
  <define name="kaijou_propfog">
    <element name="jm:Property">
      <element name="jm:Type">
        <value>濃霧</value>
      </element>
      <element name="jm:VisibilityPart">
        <oneOrMore>
          <ref name="anyElement"/>
        </oneOrMore>
      </element>
    </element>
  </define>
  <define name="kaijou_propuneri">
    <element name="jm:Property">
      <element name="jm:Type">
        <value>波</value>
      </element>
      <element name="jm:WaveHeightPart">
        <oneOrMore>
          <ref name="anyElement"/>
        </oneOrMore>
      </element>
    </element>
  </define>
  <define name="kaijou_propicing">
    <element name="jm:Property">
      <element name="jm:Type">
        <value>着氷</value>
      </element>
      <element name="jm:IcingPart">
        <oneOrMore>
          <ref name="anyElement"/>
        </oneOrMore>
      </element>
    </element>
  </define>
  <define name="kaijou_kishouyouin">
    <element name="jm:MeteorologicalInfos">
      <attribute name="type">
        <value>気象要因</value>
      </attribute>
      <element name="jm:MeteorologicalInfo">
        <element name="jm:DateTime">
          <ref name="dateTime_si"/>
        </element>
        <element name="jm:Name">
          <data type="token"/>
        </element>
        <element name="jm:Item">
          <element name="jm:Kind">
            <element name="jm:Property">
              <element name="jm:Type">
                <value>概況</value>
              </element>
              <element name="jm:SynopsisPart">
                <oneOrMore>
                  <element name="je:Synopsis">
                    <attribute name="type">
                      <value>気象要因</value>
                    </attribute>
                    <text/>
                  </element>
                </oneOrMore>
              </element>
            </element>
          </element>
          <element name="jm:Area">
            <element name="jm:Name">
              <data type="token"/>
            </element>
            <element name="jm:Code">
              <data type="positiveInteger"/>
            </element>
          </element>
        </element>
      </element>
    </element>
  </define>
  <define name="body_vpzu50_kaijou">
    <element name="jm:Body">
      <element name="jm:Notice">
        <choice>
          <value/>
          <value>警報は６時間毎に更新します。</value>
        </choice>
      </element>
      <zeroOrMore>
        <element name="jm:MeteorologicalInfos">
          <attribute name="type">
            <value>全般海上警報</value>
          </attribute>
          <oneOrMore>
            <element name="jm:MeteorologicalInfo">
              <element name="jm:DateTime">
                <ref name="dateTime_si"/>
              </element>
              <element name="jm:Item">
                <oneOrMore>
                  <element name="jm:Kind">
                    <optional>
                      <element name="jm:Name">
                        <data type="token"/>
                      </element>
                    </optional>
                    <optional>
                      <element name="jm:Code">
                        <data type="positiveInteger"/>
                      </element>
                    </optional>
                    <optional>
                      <element name="jm:Status">
                        <text/>
                      </element>
                    </optional>
                    <optional>
                      <element name="jm:ClassName">
                        <ref name="list_kaijou_classname"/>
                      </element>
                    </optional>
                    <optional>
                      <element name="jm:Condition">
                        <ref name="list_kaijou_condition"/>
                      </element>
                    </optional>
                    <optional>
                      <element name="jm:DateTime">
                        <attribute name="type">
                          <ref name="list_typh_datetime"/>
                        </attribute>
                        <ref name="dateTime_si"/>
                      </element>
                    </optional>
                    <zeroOrMore>
                      <element name="jm:Property">
                        <element name="jm:Type">
                          <ref name="list_kaijou_proptype"/>
                        </element>
                        <optional>
                          <element name="jm:WindPart">
                            <choice>
                              <element name="jm:Becoming">
                                <element name="jm:TimeModifier">
                                  <ref name="list_kaijou_timemodifier"/>
                                </element>
                                <ref name="kaijou_windpart"/>
                              </element>
                              <ref name="kaijou_windpart"/>
                            </choice>
                          </element>
                        </optional>
                        <zeroOrMore>
                          <element name="jm:WarningAreaPart">
                            <ref name="kaijou_windcircle"/>
                          </element>
                        </zeroOrMore>
                        <optional>
                          <element name="jm:ClassPart">
                            <oneOrMore>
                              <ref name="anyElement"/>
                            </oneOrMore>
                          </element>
                        </optional>
                        <optional>
                          <element name="jm:TyphoonNamePart">
                            <oneOrMore>
                              <ref name="anyElement"/>
                            </oneOrMore>
                          </element>
                        </optional>
                        <optional>
                          <element name="jm:CenterPart">
                            <oneOrMore>
                              <ref name="anyElement"/>
                            </oneOrMore>
                          </element>
                        </optional>
                        <optional>
                          <element name="jm:CoordinatePart">
                            <oneOrMore>
                              <ref name="anyElement"/>
                            </oneOrMore>
                          </element>
                        </optional>
                        <optional>
                          <element name="jm:Text">
                            <text/>
                          </element>
                        </optional>
                      </element>
                    </zeroOrMore>
                  </element>
                </oneOrMore>
                <element name="jm:Area">
                  <oneOrMore>
                    <ref name="anyElement"/>
                  </oneOrMore>
                </element>
              </element>
            </element>
          </oneOrMore>
        </element>
      </zeroOrMore>
      <zeroOrMore>
        <element name="jm:MeteorologicalInfos">
          <attribute name="type">
            <value>概況</value>
          </attribute>
          <element name="jm:MeteorologicalInfo">
            <element name="jm:DateTime">
              <ref name="dateTime_si"/>
            </element>
            <element name="jm:Item">
              <oneOrMore>
                <element name="jm:Kind">
                  <element name="jm:ClassName">
                    <choice>
                      <value>気圧系</value>
                      <value>前線</value>
                    </choice>
                  </element>
                  <zeroOrMore>
                    <element name="jm:Property">
                      <element name="jm:Type">
                        <ref name="list_kaijou_proptype2"/>
                      </element>
                      <optional>
                        <element name="jm:ClassPart">
                          <oneOrMore>
                            <ref name="anyElement"/>
                          </oneOrMore>
                        </element>
                      </optional>
                      <optional>
                        <element name="jm:CenterPart">
                          <oneOrMore>
                            <ref name="anyElement"/>
                          </oneOrMore>
                        </element>
                      </optional>
